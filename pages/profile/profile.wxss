/* pages/profile/profile.wxss */
.pro-body{
  background:url("http://127.0.0.1:3000/img/index/AFEI0rQbEAQYACCpja-fBSikpoLQBTDuBTj8AkBl.png") no-repeat;
  background-size:100% 100%;
  padding:20px 0;
}
.pro-body-css{
  padding:10px 40px;
  font-size:15px;
}

.my,.my-body{
  display:flex;
}
.my-body{
  margin:20px;
}
.my>image{
  width: 52px;
  height: 157px;
  margin:20px 10px;
  margin-left:40px;
  border-right:1px solid rgb(240, 240, 240);
  padding-right:20px;
}
.my-img>image{
  width:28px;
  height:28px;
  margin:5px;
}
.my-text{
  font-size:15px;
}
.my-text>view+view{
  font-size:12px;
  color:#aaa;
}